int libraryFine(int d1, int m1, int y1, int d2, int m2, int y2) {
    
    int y = y1 - y2, m = m1 - m2, d = d1 - d2;
    
    if(y > 0) return y * 10000;
    else if(y < 0) return 0;
    
    if(m > 0) return m * 500;
    else if(m < 0) return 0;
    
    if(d > 0) return d * 15;
    else if(d < 0) return 0;
    
    return 0;
}
